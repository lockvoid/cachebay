{
  "name": "cachebay",
  "version": "0.7.1",
  "license": "MIT",
  "author": "LockVoid",
  "description": "Framework-agnostic GraphQL cache with Relay support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown --minify",
    "coverage": "vitest --run --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepublishOnly": "pnpm build",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@codspeed/vitest-plugin": "^5.0.1",
    "@lockvoid/eslint-config": "^0.7.0",
    "@nuxt/test-utils": "^3.19.2",
    "@playwright/test": "^1.55.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.31.0",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "eslint-plugin-vue": "^10.4.0",
    "glob": "^11.0.3",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "happy-dom": "^18.0.1",
    "tailwindcss": "^4.1.13",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "peerDependencies": {
    "graphql": "^16.11.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./compiler": {
      "import": "./dist/compiler/index.js",
      "types": "./dist/compiler/index.d.ts"
    },
    "./vue": {
      "import": "./dist/adapters/vue/index.js",
      "types": "./dist/adapters/vue/index.d.ts"
    }
  },
  "files": [
    "dist",
    "docs",
    "LICENSE.md",
    "README.md"
  ],
  "dependencies": {
    "@vitejs/plugin-vue": "^6.0.1"
  }
}
