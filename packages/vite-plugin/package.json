{
  "name": "@cachebay/vite",
  "version": "0.1.0",
  "description": "Vite plugin for Cachebay GraphQL query precompilation",
  "license": "MIT",
  "author": "LockVoid",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "cachebay": "workspace:*",
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.9.2",
    "vite": "^6.0.11",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "graphql",
    "cachebay",
    "compiler",
    "precompile"
  ]
}
