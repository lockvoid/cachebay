{
  "name": "benchmarks",
  "version": "0.7.1",
  "license": "MIT",
  "author": "LockVoid",
  "private": true,
  "type": "module",
  "scripts": {
    "relay": "relay-compiler",
    "bench:api": "pnpm relay && rsbuild build && node dist/bench.cjs",
    "bench:api:baseline": "pnpm bench:api | tee .bench-results/api-baseline.txt",
    "bench:api:compare": "pnpm bench:api | tee .bench-results/api-current.txt && node scripts/compare-mitata.js",
    "bench:suite": "NODE_ENV=production vitest bench --run --mode production",
    "bench:suite:baseline": "mkdir -p .bench-results && pnpm bench:suite --outputJson .bench-results/${BENCH_NAME}-baseline.json",
    "bench:suite:compare": "pnpm bench:suite --compare .bench-results/${BENCH_NAME}-baseline.json",
    "bench:ci:baseline:api": "bash scripts/ci-baseline-api.sh",
    "bench:ci:baseline:suite": "bash scripts/ci-baseline-suite.sh",
    "bench:ci:compare:api": "bash scripts/ci-compare-api.sh",
    "bench:ci:compare:suite": "bash scripts/ci-compare-suite.sh"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@urql/core": "^6.0.1",
    "@urql/exchange-graphcache": "^7.2.4",
    "@urql/vue": "^1.4.3",
    "@vue/apollo-composable": "^4.2.1",
    "cachebay": "workspace:*",
    "cli-table3": "^0.6.5",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.10.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-relay": "^16.2.0",
    "relay-runtime": "^16.2.0",
    "serialize-javascript": "^7.0.0",
    "tinybench": "^5.0.1",
    "urql": "^4.1.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@lockvoid/eslint-config": "^0.7.0",
    "@nuxt/test-utils": "^3.19.2",
    "@rsbuild/core": "^1.5.17",
    "@swc/plugin-relay": "^9.1.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "babel-plugin-relay": "^16.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "eslint-plugin-vue": "^10.4.0",
    "glob": "^11.0.3",
    "graphql": "^16.11.0",
    "happy-dom": "^18.0.1",
    "mitata": "^1.0.34",
    "playwright": "^1.56.1",
    "relay-compiler": "^16.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite-plugin-relay": "^2.1.0",
    "vitest": "^3.2.4"
  }
}
