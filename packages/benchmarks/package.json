  "scripts": {
    "relay": "relay-compiler",
    "server:infinite-feed": "NODE_ENV=production tsx scripts/start-server-infinite-feed.ts",
    "server:nested-query": "NODE_ENV=production tsx scripts/start-server-nested-query.ts",
    "bench:infinite-feed": "NODE_ENV=production tsx scripts/start-server-infinite-feed.ts & sleep 2 && NODE_ENV=production vitest bench --run --mode production suites/infinite-feed.dom.bench.ts --config ./vitest.bench.dom.config.ts; kill %1",
    "bench:nested-query": "NODE_ENV=production tsx scripts/start-server-nested-query.ts & sleep 2 && NODE_ENV=production vitest bench --run --mode production suites/nested-query.dom.bench.ts --config ./vitest.bench.dom.config.ts; kill %1"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@urql/core": "^6.0.1",
    "@urql/exchange-graphcache": "^7.2.4",
    "@urql/vue": "^1.4.3",
    "@vue/apollo-composable": "^4.2.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.10.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-relay": "^16.2.0",
    "relay-runtime": "^16.2.0",
    "tinybench": "^5.0.1",
    "urql": "^4.1.0",
