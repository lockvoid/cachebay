    "babel-plugin-relay": "^16.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "eslint-plugin-vue": "^10.4.0",
    "glob": "^11.0.3",
    "graphql": "^16.11.0",
    "happy-dom": "^18.0.1",
    "relay-compiler": "^16.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
